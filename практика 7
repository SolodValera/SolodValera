'''
def f(m):
    pr=1
    for i in m:
        pr=pr*i
    srar=sum(m)/len(m)
    return srar,pr


for i in range(3):
    a=[int(i)for i in input().split()]
    qq=f(a)
    print(f'массив {i+1} ср ар = {qq[0]}, произведение = {qq[1]}')
'''



'''
def f(n):
    lst=[]
    for d in range(1,n+1):
        if n%d==0:
            lst.append(d)
    return lst
print(f(25))
