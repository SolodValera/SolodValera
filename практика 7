#9.1
'''
def summas(n):
    s=0
    while n!=0:
        r=n%10
        s+=r
        n=n//10
    return s



n=int(input())
cnt=0
while n>0:
    a=summas(n)
    n=n-a
    cnt+=1
print(cnt)
'''
#9.2
'''
def f(m):
    pr=1
    for i in m:
        pr=pr*i
    srar=sum(m)/len(m)
    return srar,pr


for i in range(3):
    a=[int(i)for i in input().split()]
    qq=f(a)
    print(f'массив {i+1} ср ар = {qq[0]}, произведение = {qq[1]}')
