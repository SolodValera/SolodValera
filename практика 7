#14.2
def f(x1,y1,x2,y2):
    return ((x2-x1)**2+(y2-y1)**2)**0.5

lstcord=[]
maxid=[]
for i in range(4):
    lstcord.append([int(i) for i in input().split()])
for i in range(len(lstcord)-1):#2 координаты 1 точки
    for j in range(i+1,len(lstcord)):#2 кооржинаты 2 точки
        x1=lstcord[i][0]
        y1=lstcord[i][1]
        x2=lstcord[j][0]
        y2=lstcord[j][1]
        d=f(x1,y1,x2,y2)
        maxid.append(d)
print(max(maxid))

       
    




























