'''
def summas(n):
    s=0
    while n!=0:
        r=n%10
        s+=r
        n=n//10
    return s



n=int(input())
cnt=0
while n>0:
    a=summas(n)
    n=n-a
    cnt+=1
print(cnt)
'''
