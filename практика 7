'''
def f(n):
    setd=set()#убирает двойники чисел
    for d in range(1,int(n**0.5)+1):#перебор до корня
        if n%d==0:
            setd.add(d)
            setd.add(n//d)
    return setd

lst=[]
M=int(input())
N=int(input())
for i in range(M,N+1):
    qq=f(i)
    lst.append((len(qq),i))

maxi=max(lst)[0]
rez=[i[1] for i in  lst if i[0]==maxi]
print(sorted(rez))

'''



'''
for i in lst:
    if i[0]==maxi:
    '''
        
